{
  "name": "960.gs",
  "version": "1.0",
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.js", "grid.js"],
      "css": ["grid.css"]
    }
  ],  
  "browser_action": {
      "name": "Click to toggle 960.gs grid",
      "default_icon": "icon_128.png"
  },
  "icons": { 
    "16" : "icon_16.png",
    "48" : "icon_48.png",
    "128": "icon_128.png" 
  }
}